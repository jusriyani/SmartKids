<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".DesBebek">

    <LinearLayout
        android:id="@+id/linearLayout8"
        android:layout_width="259dp"
        android:layout_height="191dp"
        android:orientation="vertical"
        android:background="#B9E3ED"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.1">

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="236dp"
            android:layout_height="149dp"
            app:srcCompat="@drawable/bebek" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="43dp"
            android:textSize="24dp"
            android:text="Bebek"
            android:textColor="@color/black"
            android:fontFamily="@font/mbold"
            android:textAlignment="center" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView"
        android:layout_width="114dp"
        android:layout_height="39dp"
        android:fontFamily="@font/mbold"
        android:text="Deskripsi"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.053"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.478" /><![CDATA[

    @font/mreg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.695"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.653" />


    ]]>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="350dp"
        android:layout_height="100dp"
        android:layout_marginTop="368dp"
        android:background="#F9C330"
        android:fontFamily="@font/mreg"
        android:text=" Bebek atau itik adalah hewan unggas yang banyak menghabiskan waktu di air. agar bisa berenang bebek menggunakan kakinya yang berselaput. bebek merupakan hewan ovipar atau bertelur, tubuh bebek dipenuhi oleh bulu kecuali moncong dan kaki,"
        android:textSize="10sp"
        android:textColor="@color/black"
        android:autoSizeMaxTextSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.695"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.009" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="163dp"
        android:layout_height="31dp"
        android:fontFamily="@font/mbold"
        android:text="Deskripsi Hewan"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.274"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="62dp"
        android:layout_height="43dp"
        android:onClick="backBebek"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.017"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.004"
        app:srcCompat="@drawable/ic_back" />
    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="63dp"
        android:layout_height="46dp"
        app:layout_constraintBottom_toBottomOf="@+id/linearLayout8"
        app:layout_constraintEnd_toEndOf="@+id/linearLayout8"
        app:srcCompat="@drawable/ic_speaker" />

</androidx.constraintlayout.widget.ConstraintLayout>